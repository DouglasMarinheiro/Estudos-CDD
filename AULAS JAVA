int a = 10;
	int b = 10;
	System.out.println (a++ + ++a); // dps do ++ = ele segura e  soma 1 para o proximo 10  \ antes do ++ ja tem valor somado antes da op 11+1 = 22
	System.out.println (b++ + b++); // 10 + 11 = 21
	
	}


}

