int c1 = 0;
		while(c1 <= 100){
			c1++;
			if(c1%2==0)
				System.out.print(c1+ " ");
		}
			
		c1 = 0;
		System.out.println("");
		while(c1 < 100){
			c1++;
			if(c1%2 !=0)
				System.out.print(c1 + " ");
			}
				
		
		}
		
		
		
	}
	
