Scanner entrada = new Scanner(System.in);
		System.out.println("Digite a palavra que deseja buscar: ");
		String palavra = entrada.nextLine();
		String txtdef = " por isso, você é indesculpável quando julga os outros, não importando quem você é. Pois, naquilo que julga o outro, você está condenando a sim mesmo, porque pratica as mesmas coisas que condena.";
		if (txtdef.contains(palavra)) {
			System.out.println("a palavra "+ palavra + "está no texto");
		}
		else {
			System.out.println("a palavra " + palavra + " não esta no texto");

	}
				
	
		}
		
		
			
